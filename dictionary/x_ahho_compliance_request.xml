<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element audit="true" extends="task" label="Request" max_length="40" name="x_ahho_compliance_request" sizeclass="1" type="collection">
        <element label="Category" mandatory="true" max_length="32" name="category" reference="x_ahho_compliance_request_category" type="reference"/>
        <element label="Pending external information" max_length="40" name="external_action_needed" type="boolean"/>
        <element label="Requester action needed" max_length="40" name="requester_action_needed" type="boolean"/>
        <element label="Request type" mandatory="true" max_length="32" name="request_type" reference="x_ahho_compliance_request_type" reference_qual="javascript:'active=true^category ='+ current.category;" type="reference" use_reference_qualifier="advanced"/>
        <element choice="1" label="Resolution code" max_length="40" name="resolution_code" type="choice">
            <choice>
                <element label="Completed" sequence="100" value="1"/>
                <element label="Pass to other department" sequence="200" value="2"/>
                <element label="No longer needed" sequence="300" value="3"/>
            </choice>
        </element>
    </element>
</database>
